
#include "library-bags.fz"

function fac (n : num) : num {
  if (n <= 1.0) then {1.0} else {n * (fac (n - 1.0))}
}

function main forall(A) (lst : list(A)) : string {
  x : = length@[A] lst;
  y : = fac x;
  num2string y
}

l : = cons(num, 1.0, cons(num, 2.0, cons(num, 3.0, nil(num))));

main@[num] l
